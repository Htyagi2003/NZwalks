@model IEnumerable<NZWalks.UI.Models.DTO.RegionDto>

@{
}



<h1 class="mt-3">Regions</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>CODE</th>
            <th>NAME</th>
        <tr>
    </thead>
    <tbody>
        @foreach (Walk in Model)
        {
            <tr>
                <td>@Walk.Id</td>
                <td>@walk.Code</td>
                <td>@Walk.Name</td>
            </tr>
        }
    </tbody>
</table>
